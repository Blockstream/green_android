<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="TextAppearance.Green.DisplayLarge" parent="TextAppearance.Material3.DisplayLarge">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">36dp</item>
        <item name="lineHeight">40dp</item>
        <item name="android:lineHeight" tools:ignore="NewApi">40dp</item>
    </style>

    <style name="TextAppearance.Green.DisplayMedium" parent="TextAppearance.Material3.DisplayMedium">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">30dp</item>
        <item name="lineHeight">36dp</item>
        <item name="android:lineHeight" tools:ignore="NewApi">36dp</item>
    </style>

    <style name="TextAppearance.Green.DisplaySmall" parent="TextAppearance.Material3.DisplaySmall">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">28dp</item>
        <item name="lineHeight">33dp</item>
        <item name="android:lineHeight" tools:ignore="NewApi">33dp</item>
    </style>

    <style name="TextAppearance.Green.HeadlineLarge" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">26dp</item>
        <item name="lineHeight">28dp</item>
        <item name="android:lineHeight" tools:targetApi="p">28dp</item>
    </style>

    <style name="TextAppearance.Green.HeadlineMedium" parent="TextAppearance.Material3.HeadlineMedium">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">24dp</item>
        <item name="lineHeight">26dp</item>
        <item name="android:lineHeight" tools:targetApi="p">26dp</item>
    </style>

    <style name="TextAppearance.Green.HeadlineSmall" parent="TextAppearance.Material3.HeadlineSmall">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">22dp</item>
        <item name="lineHeight">24dp</item>
        <item name="android:lineHeight" tools:targetApi="p">24dp</item>
    </style>

    <style name="TextAppearance.Green.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="TextAppearance.Green.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="TextAppearance.Green.TitleSmall" parent="TextAppearance.Material3.TitleSmall">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="TextAppearance.Green.BodyLarge" parent="TextAppearance.Material3.BodyLarge">
