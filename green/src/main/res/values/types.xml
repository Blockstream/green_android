        <item name="android:lineHeight" tools:targetApi="p">20dp</item>
    </style>

    <style name="TextAppearance.Green.LabelMedium" parent="TextAppearance.Material3.LabelMedium">
        <item name="android:textSize">12dp</item>
        <item name="lineHeight">16dp</item>
        <item name="android:lineHeight" tools:targetApi="p">16dp</item>
    </style>

    <style name="TextAppearance.Green.LabelSmall" parent="TextAppearance.Material3.LabelSmall">
        <item name="android:textSize">9dp</item>
        <item name="lineHeight">9dp</item>
        <item name="android:lineHeight" tools:targetApi="p">9dp</item>
    </style>

    <style name="TextAppearance.Green.Overline" parent="TextAppearance.Green.LabelMedium">
        <item name="android:textColor">@color/color_on_surface_emphasis_medium</item>
    </style>

    <style name="TextAppearance.Green.Subtitle" parent="TextAppearance.Green.BodyMedium">
        <item name="lineHeight">22dp</item>
        <item name="android:lineHeight" tools:targetApi="p">22dp</item>
    </style>


</resources>
